{
  "name": "verisurf-nightly",
  "version": "2.0.2",
  "main": "public/Main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/tytech3/Verisurf-Nightly.git"
  },
  "homepage": "./",
  "dependencies": {
    "@auth0/s3": "^1.0.0",
    "@material-ui/core": "^4.8.3",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/lab": "^4.0.0-alpha.41",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "apexcharts": "^3.15.2",
    "electron-updater": "^4.2.0",
    "material-ui-icons": "^1.0.0-beta.36",
    "react": "^16.12.0",
    "react-apexcharts": "^1.3.6",
    "react-dom": "^16.12.0",
    "react-reveal": "^1.2.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "regedit": "^3.0.3",
    "request": "^2.88.2",
    "typeface-roboto": "0.0.75"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-start": "electron .",
    "preelectron-pack": "npm run build",
    "electron-pack": "electron-builder --win -p always -c.extraMetadata.main=build/Main.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@auth0/s3": "^1.0.0",
    "aws-sdk": "^2.607.0",
    "electron": "^9.4.0",
    "electron-builder": "^21.2.0",
    "electron-updater": "^4.2.0",
    "react-reveal": "^1.2.2",
    "regedit": "^3.0.3",
    "typescript": "^3.7.5"
  },
  "build": {
    "publish": [
      {
        "provider": "github",
        "owner": "tytech3",
        "repo": "Verisurf-Nightly"
      }
    ],
    "appId": "com.verisurfsoftware.nightly",
    "files": [
      "build/",
      "node_modules/**/*"
    ],
    "win": {
      "requestedExecutionLevel": "requireAdministrator"
    },
    "directories": {
      "buildResources": "assets"
    },
    "extraResources": [
      {
        "from": "node_modules/regedit/vbs",
        "to": "regedit/vbs",
        "filter": [
          "**/*"
        ]
      }
    ]
  }
}
